{
  "presets": [
   [ "next/babel", {
      "styled-jsx": {
        "plugins": [
          "styled-jsx-plugin-postcss",
          ["styled-jsx-plugin-sass", {
              "sassOptions": {
                "includePaths": ["client/styles"],
                "precision": 2,
              },
            },
          ],
        ],
      },
   }],
    "@babel/preset-flow"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {
      "legacy": false,
      "decoratorsBeforeExport": false,
    }],
    "@babel/plugin-proposal-class-properties",
  ],
}
