{
  "private": true,
  "name": "emailapp",
  "version": "1.0.0",
  "description": "A mock email app frontend and backend",
  "main": "register.js",
  "scripts": {
    "dev": "cross-env NODE_ENV='development' nodemon -w .postcssrc.js -w next.config.js -w server -w config -w client/routes.js node register.js -f ./server/index.js",
    "start": "cross-env NODE_ENV='production' node ./build/server/index.js",
    "build:client": "next build client",
    "build:server": "cross-env BABEL_ENV=node babel server -d build/server --config-file ./.babelrc.server.js --no-babelrc",
    "fix": "npx eslint \"client/**/*.js\" \"server/**/*.js\" *.js --fix -c .eslintrc.js",
    "lint": "npx eslint \"client/**/*.js\" \"server/**/*.js\" *.js -c .eslintrc.js",
    "db:setup": "knex migrate:latest && knex seed:run",
    "docker:build": "docker-compose build --no-cache",
    "docker:up": "docker-compose -f docker-compose.yml up",
    "docker:stop": "docker stop emailapp_web emailapp_pg",
    "docker:ssh": "docker exec -it emailapp_web sh -l",
    "docker:rm": "docker rm emailapp_web emailapp_pg"
  },
  "_moduleAliases": {
    "@root": "./",
    "@": "server",
    "@client": "client"
  },
  "directories": {
    "lib": "server",
    "test": "test/"
  },
  "engines": {
    "node": "9.0.0",
    "npm": ">= 3.0.0"
  },
  "author": "Anthony Koch",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "9.1.5",
    "cross-env": "5.2.0",
    "knex": "0.15.2",
    "mobx": "5.1.2",
    "mobx-react": "5.2.8",
    "next": "7.0.0",
    "next-routes": "1.4.2",
    "normalize.css": "8.0.0",
    "objection": "1.3.0",
    "pg": "7.4.3",
    "postcss-import": "^12.0.0",
    "postcss-url": "^8.0.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "validatorjs": "3.14.2",
    "yargs": "12.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@feathersjs/configuration": "2.0.3",
    "@feathersjs/errors": "3.3.3",
    "@feathersjs/express": "1.2.6",
    "@feathersjs/feathers": "3.2.2",
    "@feathersjs/socketio": "3.2.5",
    "babel-eslint": "9.0.0",
    "compression": "1.7.3",
    "cors": "2.8.4",
    "css-loader": "1.0.0",
    "cssnano": "4.1.0",
    "eslint": "5.6.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "4.0.0",
    "file-loader": "2.0.0",
    "flow-bin": "0.81.0",
    "glob": "7.1.3",
    "helmet": "3.13.0",
    "mini-css-extract-plugin": "0.4.3",
    "mocha": "5.2.0",
    "module-alias": "2.1.0",
    "node-sass": "4.9.3",
    "nodemon": "1.18.4",
    "postcss-advanced-variables": "^2.3.3",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.0",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "serve-favicon": "2.5.0",
    "style-loader": "0.23.0",
    "styled-jsx-plugin-postcss": "0.1.3",
    "to-string-loader": "1.1.5",
    "url-loader": "1.1.1",
    "winston": "3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
