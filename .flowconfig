[ignore]
.*/node_modules/jsonlint/.*
.*/node_modules/bcryptjs/.*
.*/.next/.*

[include]
client

[libs]
flow-typed/

[lints]

[options]
esproposal.optional_chaining=enable
esproposal.decorators=ignore
module.name_mapper='^@app\/\(.*\)$' -> '<PROJECT_ROOT>/client/\1'
module.name_mapper='^@shared\/\(.*\)$' -> '<PROJECT_ROOT>/shared/\1'
module.name_mapper='^@server\/\(.*\)$' -> '<PROJECT_ROOT>/server/\1'
module.name_mapper='^@root\/\(.*\)$' -> '<PROJECT_ROOT>/\1'

[strict]
